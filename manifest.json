{
  "manifest_version": 3,
  "name": "React Chrome Extension",
  "version": "1.0.0",
  "description": "This is a Chrome extension built with React and JavaScript",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://api.open-meteo.com/v1/forecast"
  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/index.js"]
    }
  ]
}
