{
  "manifest_version": 3,
  "name": "React Chrome Extension",
  "version": "1.0.0",
  "description": "This is a Chrome extension built with React and JavaScript",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["http://api.carsxe.com/*"], 
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
